			data: {
				sessionid: g_sessionID,
				'currency': 3,
				appid: apps[i],
				'market_hash_name': 'MarketItem',
				'price_total': 3,
				'quantity': 1,
			}
		}).always(function() {
			loaded++;

			modal.Dismiss();

			if (loaded >= total) {
				ShowAlertDialog('All done!', 'Enjoy.');
			} else {
				modal = ShowBlockingWaitDialog('Executing...',
					'Loaded ' + loaded + '/' + total + '. Ignore all the errors, let it finish.');

				i++;

				ExecuteRequest();
			}
		});
	};

